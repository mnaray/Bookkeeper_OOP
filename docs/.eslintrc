{
    "extends": ["plugin:@docusaurus/recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "env": {
        "es6": true
    },
    "plugins": ["prettier"],
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "double"],
        "prettier/prettier": ["error"]
    }
}
